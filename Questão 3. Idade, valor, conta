   inteiro: idade;
   inteiro: deposito;
   inteiro: conta;

   escreva("Digite sua idade: ");
   leia(idade);
   escreva("Digite o valor a depositar: ");
   leia(deposito);
   escreva("Digite o numero da conta: ");
   leia(conta);

   escreval("Idade: ", idade);
   escreval("Deposito: ", deposito);
   escreval("Conta: ", conta);
